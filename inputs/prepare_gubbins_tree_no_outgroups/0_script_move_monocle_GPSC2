mkdir $(pwd)/data_fasta
for file in $(find $(pwd)/*/ -type f -name "*.fa"); do
  mv -v "$file" $(pwd)/data_fasta;
  done

for file in $(find $(pwd)/data_fasta/ -type f -name "*.fa"); do
  cp -v "$file" /home/ron/Strep_Indo/raw_data/serotype_1/prepare_rerun_gubbins_tree_suggestions_19F_as_outgroups_ww_Indo;
  done

mkdir $(pwd)/data_gff
for file in $(find $(pwd)/*/ -type f -name "*.gff"); do
  mv -v "$file" $(pwd)/data_gff;
  done



